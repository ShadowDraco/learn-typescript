{
  "name": "server-side-game",
  "version": "1.0.0",
  "description": "Learn typescript and realtime processing for something like a game",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "jest --verbose --coverage",
    "test-watch": "jest --watchAll --verbose --coverage",
    "init:config": "sequelize init:config",
    "db:create": "sequelize db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShadowDraco/server-side-game.git"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShadowDraco/server-side-game/issues"
  },
  "homepage": "https://github.com/ShadowDraco/server-side-game#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "socket.io": "^4.6.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "concurrently": "^8.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
